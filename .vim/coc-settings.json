{
  "java.errors.incompleteClasspath.severity": "ignore",
  "coc.preferences.rootPatterns": [
    ".git",
    ".hg",
    ".projections.json",
    "shell.nix"
  ],
  "languageserver": {
    "bash": {
      "ignoredRootPaths": [
        "~"
      ],
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ]
    },
    "haskell": {
      "command": "haskell-language-server-wrapper",
      "args": ["--lsp"],
      "filetypes": [
        "hs",
        "lhs",
        "haskell",
        "lhaskell"
      ],
      "rootPatterns": [
        "*.cabal",
        "stack.yaml",
        "cabal.project",
        "package.yaml",
        "hie.yaml"
      ],
      "settings": {
      },
      "initializationOptions": {
      }
    }
  }
}
